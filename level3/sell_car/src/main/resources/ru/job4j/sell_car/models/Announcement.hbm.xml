<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping>
    <class name="ru.job4j.sell_car.models.Announcement" table="announcements">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <many-to-one name="user" column="user_id" class="ru.job4j.sell_car.models.User"/>
        <many-to-one name="car" column="car_id" class="ru.job4j.sell_car.models.Car"/>
        <property name="sell" column="sell"/>
    </class>
</hibernate-mapping>